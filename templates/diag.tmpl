<!doctype html>
<html lang="en">
{{template "head" .}}
<body>
  {{template "header" .}}
  <div class="splash-container">
    <div class="splash">
    url cache hits: {{ .CacheHit }} <br />
    url cache miss: {{ .CacheMiss }} <br />
    url cache len:  {{ .CacheLen }}
    </div>
  </div>
  <div class="content-wrapper">
    {{template "submit" .}}
    {{template "webscale" .}}
    {{template "testimonials" .}}
    {{template "footer" .}}
  </div>
{{template "scripts" .}}
</body>
</html>